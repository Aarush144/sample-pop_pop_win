{
  "manifest_version": 2,
  "name": "Pop, Pop, Win!",
  "description" : "A new spin on the classic Minesweeper game with darts and balloons."
  "version": "1.9.1",
  "minimum_chrome_version": "27",
  "icons": {
  	"16": "icons/icon_16.png",
  	"48": "icons/icon_48.png",
  	"128": "icons/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [ "storage" ]
}
